# Gemini AI
GEMINI_API_KEY=your_gemini_api_key_here

# Database (PostgreSQL)
DATABASE_URL=postgresql://user:password@localhost:5432/ai_media_db
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ai_media_db
DB_USER=your_db_user
DB_PASSWORD=your_db_password

# Application
ENVIRONMENT=development
LOG_LEVEL=INFO

# Scraping
MAX_NEWS_PER_SOURCE=None
SLEEP_BETWEEN_NEWS=3
SLEEP_BETWEEN_SOURCES=3
TIMEOUT=30

# Scheduler
ENABLE_SCHEDULER=False
SCRAPING_INTERVAL_HOURS=1

EXPORT_MODE=postgres